#include<bits/stdc++.h>                          /// cpp + ctrl+j
#define ll long long
#define dubb long double
#define pi acos(-1)
using namespace std;
const ll N=1000;
char s[N];

int main()
{
    cin.getline(s, N);
    int len = strlen(s);
    for (int i = 0; i < len; i++)
    {
        if (s[i] == '\\')
        {
            break;
        }
        cout << s[i];
    }
    cout << '\n';




    return 0;
}
